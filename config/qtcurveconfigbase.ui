       <item row="0" column="0" colspan="4" >
        <widget class="QCheckBox" name="gtkComboMenus" >
         <property name="whatsThis" >
          <string>&lt;html>&lt;head>&lt;meta name="qrichtext" content="1" />&lt;style type="text/css">
p, li { white-space: pre-wrap; }
&lt;/style>&lt;/head>&lt;body style=" font-family:'Verdana'; font-size:9pt; font-weight:400; font-style:normal;">
&lt;p style=" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;">In Gtk, the list that is attached to a combo-box appears in the same style as a popup-menu. KDE4 has the ability to mimic this look.&lt;/p>&lt;/body>&lt;/html></string>
         </property>
         <property name="text" >
          <string>Gtk style comboboxes (KDE4)</string>
         </property>
        </widget>
       </item>
       <item row="1" column="0" colspan="4" >
